{
  "hosting": [
    {
      "site": "fintstatustest",
      "public": "build",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/proxy/**",
          "function": "proxy"
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "site": "fintstatus",
      "public": "build",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/proxy/**",
          "function": "proxy"
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}